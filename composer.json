{
  "name":        "ingenerator/tokenista",
  "description": "Simple signed and expiring token generator and validator - for password reset, CSRF, authentication, whatever",
  "keywords":    ["auth", "token", "csrf"],
  "homepage":    "https://github.com/ingenerator/tokenista",
  "type":        "library",
  "license":     "BSD-3-Clause",
  "authors":     [
    {
      "name":     "Andrew Coulton",
      "email":    "andrew@ingenerator.com",
      "homepage": "https://www.ingenerator.com"
    }
  ],
  "require": {
    "php":         "^7.4",
    "ext-openssl": "*",
    "lib-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "suggest": {
  },
  "support": {
    "issues": "https://github.com/ingenerator/tokenista/issues",
    "source": "https://github.com/ingenerator/tokenista"
  },
  "autoload": {
    "psr-4": {
      "Ingenerator\\": "src/Ingenerator"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ingenerator\\Tests\\": "tests/Ingenerator"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
